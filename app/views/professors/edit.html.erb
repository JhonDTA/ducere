<% provide(:title, t('edit_resource', resource: t('activerecord.models.professor'))) %>
<% provide :card_title, t('activerecord.models.professor') %>

<%= render 'shared/forms/form_card' do %>
  <%= render 'form', professor: @professor %>
  <% content_for :back_button do %>
    <%= link_to t('show'), @professor %>
    <%= link_to t('back'), professors_path %>
  <% end %>
<% end %>
