<% provide(:title, t('edit_resource', resource: t('activerecord.models.student'))) %>
<% provide :card_title, t('activerecord.models.student') %>

<%= render 'shared/forms/form_card' do %>
  <%= render 'form', student: @student %>
  <% content_for :back_button do %>
    <%= link_to t('show'), @student %>
    <%= link_to t('back'), students_path %>
  <% end %>
<% end %>
