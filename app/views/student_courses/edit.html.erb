<% provide(:title, t('edit_resource', resource: t('activerecord.models.student_course'))) %>
<% provide :card_title, t('activerecord.models.student_course') %>

<%= render 'shared/cards/form_card' do %>
  <%= render 'form', student_course: @student_course %>
  <% content_for :card_footer do %>
    <%= link_to t('show'), @student_course, class: 'btn btn-sm btn-info' %>
    <%= link_to t('back'), student_courses_path, class: 'btn btn-sm btn-danger' %>
  <% end %>
<% end %>
