<% provide(:title, t('edit_resource', resource: t('activerecord.models.course_mark'))) %>
<% provide :card_title, t('activerecord.models.course_mark') %>

<%= render 'shared/cards/form_card' do %>
  <%= render 'form', course_mark: @course_mark %>
  <% content_for :card_footer do %>
    <%= link_to t('show'), @course_mark, class: 'btn btn-sm btn-info' %>
    <%= link_to t('back'), course_marks_path, class: 'btn btn-sm btn-danger' %>
  <% end %>
<% end %>
