<% provide(:title, t('edit_resource', resource: t('activerecord.models.classroom'))) %>
<% provide :card_title, t('activerecord.models.classroom') %>

<%= render 'shared/forms/form_card' do %>
  <%= render 'form', classroom: @classroom %>
  <% content_for :back_button do %>
    <%= link_to t('show'), @classroom %> |
    <%= link_to t('back'), classrooms_path %>
  <% end %>
<% end %>
