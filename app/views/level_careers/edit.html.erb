<% provide(:title, t('edit_resource', resource: t('activerecord.models.level_career'))) %>
<% provide :card_title, t('activerecord.models.level_career') %>

<%= render 'shared/cards/form_card' do %>
  <%= render 'form', level_career: @level_career %>
  <% content_for :card_footer do %>
    <%= link_to t('show'), @level_career, class: 'btn btn-sm btn-info' %>
    <%= link_to t('back'), level_careers_path, class: 'btn btn-sm btn-danger' %>
  <% end %>
<% end %>
