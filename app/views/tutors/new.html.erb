<% provide(:title, t('new_resource', resource: t('activerecord.models.tutor'))) %>
<% provide :card_title, t('activerecord.models.tutor') %>

<%= render 'shared/cards/form_card' do %>
  <%= render 'form', tutor: @tutor %>
  <% content_for :card_footer do %>
    <%= link_to tutors_path, class: 'btn btn-sm btn-danger' do %>
      <%= icon('fas', 'undo-alt') %> <%= t('back') %>
    <% end %>
  <% end %>
<% end %>
